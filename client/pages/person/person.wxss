.faq-box nav .nav-list {
    width: 100%;
}
 .nav-list>view {
    width: 100%;
    height: 2.54666667rem;
    line-height: 2.54666667rem;
    border-bottom: 1px solid #F3F5F7;
    margin: 0 auto;
    position: relative;
}
view.a {
    display: block;
    width: 100%;
    height: 2.54666667rem;
    line-height: 2.54666667rem;
    padding-left: 4%;
    padding-right: 4%;
    overflow: hidden;
}
.cate-title {
    width: 90px;
    float: left;
    font-size: 16px;
    color: #2B333B;
    letter-spacing: 0;
    text-align: left;
    margin-left: 14px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.rt_container {
    width: 40px;
    float: right;
}
.imwap-arrow_r {
    width: 40px;
    float: right;
}
.rt_container image {
    width: 30px;
    height: 30px;
    border-radius: 250px;
    margin-top: 5px;
}